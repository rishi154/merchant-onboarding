# Google Cloud Configuration for VertexAI
GOOGLE_CLOUD_PROJECT=steam-collector-339717
GOOGLE_CLOUD_LOCATION=us-central1
# GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# Authentication Options (choose one):
# Option 1: Service Account Key (above)
# Option 2: Application Default Credentials (easiest)
#   Run: gcloud auth application-default login

# Google Document AI Configuration
GOOGLE_DOC_AI_PROCESSOR_ID=your-processor-id

# GlobalPayments Configuration
GLOBALPAYMENTS_URL=https://apis.globalpay.com/ucp
GLOBALPAYMENTS_APP_ID=your-app-id
GLOBALPAYMENTS_APP_KEY=your-app-key
GLOBALPAYMENTS_ACCOUNT_ID=your-account-id

# Agent Configuration
AGENT_TEMPERATURE=0.1
AGENT_MAX_TOKENS=2048
ENABLE_AGENT_MEMORY=true

# Integration Mock/Real Configuration (true=mock, false=real)
MOCK_CREDIT_BUREAUS=true
MOCK_KYC_PROVIDERS=true
MOCK_BANKING_APIS=true
MOCK_GOVERNMENT_DATABASES=true
MOCK_GLOBALPAYMENTS=true

